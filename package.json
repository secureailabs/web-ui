{
  "name": "@secureailabs/web-ui",
  "repository": "git://github.com/secureailabs/web-ui.git",
  "dependencies": {
    "@types/react-grid-layout": "^1.3.1",
    "change-case": "^4.1.2",
    "css-loader": "^6.5.1",
    "eqcss": "^1.9.2",
    "javascript-time-ago": "^2.3.10",
    "react": "^17.0.2",
    "react-avatar": "^4.0.0",
    "react-grid-layout": "^1.3.3",
    "react-hook-form": "^7.25.3",
    "react-icons": "^4.3.1",
    "react-resizable": "^3.0.4",
    "react-scripts": "5.0.0",
    "react-table": "^7.7.0",
    "react-time-ago": "^7.1.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "ttsc --project tsconfig.prod.json && cp package.json dist && npm run build:css && cp -r src/assets dist/assets",
    "compile:sass": "node-sass src/sass/main.scss dist/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' dist/css/style.comp.css -o dist/css/style.prefix.css",
    "compress:css": "node-sass dist/css/style.prefix.css dist/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "semantic-release": "semantic-release"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^9.0.0",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/builder-webpack5": "^6.4.13",
    "@storybook/manager-webpack5": "^6.4.13",
    "@storybook/node-logger": "^6.4.13",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.13",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/react-table": "^7.7.9",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin": "^1.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "react-dom": "^17.0.2",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "sass": "^1.49.0",
    "sass-loader": "^12.4.0",
    "semantic-release": "^19.0.2",
    "storybook-addon-sass-postcss": "^0.1.0",
    "style-loader": "^3.3.1",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.4.2",
    "typescript-transform-paths": "^2.2.4",
    "web-vitals": "^2.1.0",
    "webpack": "^5.66.0"
  }
}
